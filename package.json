{
  "name": "dnod",
  "version": "0.0.7",
  "description": "The ultimate node editor to become",
  "main": "index.js",
  "scripts": {
    "start": "dsrv --path=?dev",
    "server": "nodemon --watch server server/index.js",
    "build": "rollup -c",
    "docs": "jsdoc src -r -d docs",
    "release": "npm run build && terser build/index.js --compress --mangle --module --output build/index.js",
    "bump": "npm version patch && npm publish",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Damien Mortini",
  "license": "ISC",
  "dependencies": {
    "dlib": "0.0.97",
    "dlmn": "0.0.7",
    "ws": "^6.2.1"
  },
  "devDependencies": {
    "dsrv": "0.0.3",
    "jsdoc": "^3.5.5",
    "rollup": "^1.9.0",
    "terser": "^3.17.0"
  }
}
