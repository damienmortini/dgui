{
  "name": "@damienmortini/graph",
  "version": "0.2.4",
  "description": "The ultimate node editor to become",
  "main": "index.js",
  "scripts": {
    "start": "server --path=?dev",
    "build": "rollup -c",
    "release": "npm run build && terser build/index.js --compress --mangle --module --output build/index.js",
    "docs": "jsdoc -c jsdoc.config.json",
    "server": "nodemon --watch server server/index.js",
    "bump-patch": "npm version patch && npm publish --access=public",
    "bump-minor": "npm version minor && npm publish --access=public",
    "bump-major": "npm version major && npm publish --access=public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Damien Mortini",
  "license": "ISC",
  "dependencies": {
    "@damienmortini/elements": "^0.2.3",
    "@damienmortini/lib": "^0.2.0",
    "ws": "^7.1.0"
  },
  "devDependencies": {
    "@damienmortini/server": "^1.0.2",
    "jsdoc": "^3.6.3",
    "rollup": "^1.19.4",
    "terser": "^4.1.4"
  }
}
